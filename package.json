{
  "name": "ng-form-builder",
  "version": "1.0.0",
  "description": "simple json based form builder engine using angular 1.x ",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --hot --inline",
    "start:old": "budo src/index.js:index.js --dir='src' --open --pushstate --base  --live -- -t node-scssify -t babelify -t stringify -t browserify-css",
    "builder:serve": "budo src/index.js --open --live -- -t babelify -t stringify -t browserify-css",
    "serve": "run-p builder:serve build",
    "test": "karma test",
    "build": "browserify -t babelify -t stringify -t browserify-css src/form-builder.js>build/ng-formbuilder.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dimpu/ngFormBuilder.git"
  },
  "keywords": [
    "angular",
    "formbuilder",
    "forms",
    "json"
  ],
  "author": "Dimpu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dimpu/ngFormBuilder/issues"
  },
  "homepage": "https://github.com/dimpu/ngFormBuilder#readme",
  "devDependencies": {
    "angular-hmr": "^0.2.0",
    "angular-hot-loader": "^0.9.10",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.1.18",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "browserify-css": "^0.8.1",
    "budo": "^6.0.4",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.28.7",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "json-loader": "^0.5.7",
    "karma": "^1.7.0",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-mocha": "^0.2.2",
    "mocha": "^2.5.3",
    "node-sass": "^4.5.3",
    "node-scssify": "^1.0.0",
    "npm-run-all": "^4.0.2",
    "sass-loader": "^6.0.6",
    "scss-loader": "0.0.1",
    "stringify": "^3.1.0",
    "style-loader": "^0.18.2",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.8.2"
  },
  "dependencies": {
    "@uirouter/angularjs": "^1.0.6",
    "angular": "^1.6.6"
  }
}
